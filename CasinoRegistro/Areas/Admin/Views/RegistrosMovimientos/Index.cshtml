


@{
    ViewData["Title"] = "Lista de Movimientos";
}

<div class="container mt-4">
    <div class="row">
        <div class="col-8">
            <h2 class="text-primary">Lista de Movimientos</h2>
        </div>
        <div class="col-4 text-right">
            <a asp-action="CreateFichas" class="btn btn-primary">  <i class="fa-solid fa-plus"></i> Ingreso de Fichas</a>
            <a asp-action="CreateDinero" class="btn btn-primary"><i class="fa-solid fa-plus"></i> Ingreso de Dinero</a>
        </div>
    </div>

    <br />
    <br />
    <table id="tblRegistros" class="table table-striped table-bordered" style="width: 100%">
        <thead>
            <tr>               
                <th>Cajero</th>                
                <th>Fecha</th>
                <th align="center">Acciones</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/registro.js"></script>
}
